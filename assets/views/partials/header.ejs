<div class="container">
    <header>
        <div >
            <a href="/" title="Home">
                <img src="/img/logo/logo.png" class="page_logo img-responsive">
            </a>
        </div>
        <div class="navbar-header">
            <button id="nav_burger_btn" type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse" aria-expanded="false">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        <div class="collapse navbar-collapse header_div" id="navbar-collapse">
            <div class="row">
                <div class="col-xs-12 navbar_div">
                    <ul class="nav navbar-nav navbar-left pulls-left" id="header_list">
                        <li role="presentation">
                            <a class="menu_link openBold" href="/">
                                <span class="glyphicon glyphicon-home"></span>
                                Forside
                            </a>
                        </li>
                        <% menu_categories.forEach(category => { %>
                            <li role="presentation">
                                <a class="menu_link openBold" href=<%= "/" + category.query_name %>>
                                    <%= category.editor_name %>
                                </a>
                            </li>
                        <% })%>
                        <li role="presentation">
                            <a class="menu_link openBold" href="/archive">Arkiv</a>
                        </li>
                        <li role="presentation">
                            <a class="menu_link openBold" href="/contact">Kontakt</a>
                        </li>
                        <li role="presentation">
                            <a class="menu_link openBold" href="/editorial">Redaktionen</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </header>
</div>
